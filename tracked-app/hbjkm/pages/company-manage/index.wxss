.swiper-tab {
    width: 100%;
    text-align: center;
    line-height: 80rpx;
}

.swiper-tab-list {
    font-size: 30rpx;
    display: inline-block;
    border-bottom: 1px solid #ddd;
    width: 33.33%;
    color: #777777;
}

.mycheck {
    color: #4293F4;
    font-size: 36rpx;
}

.on {
    color: #4293F4;
    border-bottom: 3rpx solid #4293F4;
}

.swiper-box {
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
    background: #F6F6F6;
}

.swiper-box view {
    text-align: center;
}

swiper-item {
    padding-top: 20rpx;
}

.listitem {
    width: 100%;
    height: 130rpx;
    overflow: hiden;
    padding: 20rpx 10rpx;
    background: #fff;
}

swiper-item .listitemcont {
    width: 90%;
    height: 130rpx;
    border-bottom: 1px solid #ddd;
    text-align: left;
    overflow: hidden;
    margin: 0px auto;
}

.listleft {
    float: left;
    width: 70%;
    text-align: left;
}

.listrig {
    float: right;
    width: 30%;
    text-align: right;
    padding-top: 36rpx;
    padding-right: 22rpx;
    box-sizing: border-box;
}

.listrig button {
    background: #4393F4;
    color: #fff;
    text-align: center;
    width: 60rpxk;
}

.swiper-box view {
    text-align: left;
}

.swiper-box .listrig {
    text-align: right;
}

.listitemconted {
    height: 160rpx;
}

.listitemconted .listitemcont {
    height: 160rpx;
}

.namecont {
    font-size: 34rpx;
    font-weight: bold;
    margin-bottom: 20rpx;
}

.contedall .listleft {
    width: auto;
}

.contedall .listrig {
    width: 220rpx;
    padding-right: 0rpx;
    padding-top: 5rpx;
}

.name {
    margin-right: 30rpx;
}

.swiper-tab {
    height: 80rpx;
}

.swiper-box .more {
    background: #F6F6F6;
    text-align: center;
    height: 300rpx;
    width: 100%;
    padding-top: 100rpx;
}