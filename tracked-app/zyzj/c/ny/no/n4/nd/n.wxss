.npl {
    background-color: var(--white);
    border-radius: var(--border-radius-l);
    color: var(--text-main);
    font-size: 30rpx;
    font-weight: var(--font-weight-bold);
    line-height: 30rpx;
    margin-bottom: 16rpx;
    padding: 36rpx 24rpx;
}

.npq {
    display: flex;
    justify-content: space-between;
}

.npr {
    color: var(--text-third);
    font-size: 26rpx;
    font-weight: var(--font-weight-normal);
    line-height: 26rpx;
    margin-top: 16rpx;
}

.npx {
    justify-content: space-between;
}

.np8,.npx {
    display: flex;
}

.np8 {
    margin-top: 32rpx;
}

.npj {
    color: var(--text-main);
    display: inline-block;
    font-size: 32rpx;
}

.ny4 {
    color: #ff8000;
    font-size: 24rpx;
    font-weight: 400;
    margin-top: 20rpx;
}

.nye {
    color: #666;
    line-height: 38rpx;
}

.nye,.nyn {
    font-size: 26rpx;
}

.nyn {
    color: #999;
    text-align: right;
}

.nyc {
    align-items: center;
    color: var(--text-main);
    display: flex;
    font-size: 24rpx;
    height: 32rpx;
}

.nyz {
    height: 30rpx;
    width: 36rpx;
}