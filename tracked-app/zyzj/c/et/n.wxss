.n40 {
    align-items: center;
    background-color: #fff;
    display: flex;
    height: 100vh;
    justify-content: center;
    left: 0;
    position: fixed;
    top: 0;
    width: 100vw;
    z-index: 9000;
}

.n40 .n4l {
    height: 300rpx;
    left: 50%;
    position: absolute;
    top: 45%;
    transform: translate(-50%,-50%);
    width: 300rpx;
}

.n40.n4q {
    background-color: #f5f5f5;
}