.cek {
    align-content: center;
    bottom: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 999999;
}

.cep {
    background-color: rgba(51,51,51,.9);
    border-radius: 16rpx;
    display: block;
    margin: 0 auto;
    max-width: 484rpx;
    min-width: 154rpx;
    padding: 48rpx;
    text-align: center;
}

.cey {
    color: #fff;
    font-size: 32rpx;
}