.exd {
    align-items: center;
    background-color: var(--marketingColor);
    border-radius: 50rpx;
    box-sizing: border-box;
    color: var(--white);
    display: flex;
    font-weight: var(--font-weight-bold);
    justify-content: center;
    padding: 0 8rpx;
}

.exd.e9l {
    height: 34rpx;
    line-height: 34rpx;
    min-width: 34rpx;
}

.exd.ext {
    font-size: 20rpx;
    height: 32rpx;
    line-height: 32rpx;
    min-width: 32rpx;
}