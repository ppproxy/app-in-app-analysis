.nsq {
    align-items: center;
    display: flex;
    text-align: left;
}

.nsq .nsr {
    background-color: rgba(0,0,0,.9);
    border-radius: 100%;
    height: 44rpx;
    margin-right: 10rpx;
    width: 44rpx;
}

.nsq .nsx {
    color: rgba(0,0,0,.9);
    font-size: 28rpx;
    font-weight: 700;
}

.ns8 {
    font-size: 34rpx;
}

.ns8 .nsj {
    align-items: center;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    display: flex;
    justify-content: space-between;
    padding: 30rpx 0;
}

.ns8 .nf4 {
    background: #fff;
    color: #576b95;
    font-size: 28rpx;
    padding: 0;
    text-align: right;
    width: 65%;
}

.ns8 .nfe {
    width: 100%;
}

.ns8 .nfe,.ns8 .nfn {
    font-size: 30rpx;
    text-align: left;
}

.ns8 .nfn {
    align-items: center;
    display: inline-flex;
    min-width: 120rpx;
}

.ns8 .nfc {
    color: #000;
    display: inline-block;
    height: 38rpx;
    top: 0;
    width: 40rpx;
}

.nfz {
    display: flex;
    margin-top: 64rpx;
}

.nfz .nf5 {
    background: rgba(0,0,0,.05);
    border: 0;
    border-radius: 7rpx;
    color: #303030;
    flex: 1;
    font-size: 30rpx;
    font-weight: 500;
    letter-spacing: 2rpx;
    text-align: center;
}

.nfz .nf5::after,.nfz .nfw::after {
    border: none;
}

.nfz .nfw {
    border: 0;
    border-radius: 7rpx;
    color: #fff;
    flex: 1;
    font-size: 30rpx;
    font-weight: 500;
    letter-spacing: 2rpx;
    margin-left: 20rpx;
    text-align: center;
}