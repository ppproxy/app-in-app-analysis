.cl4 {
    background-color: var(--white);
    border-radius: var(--border-radius-l);
    box-sizing: border-box;
    margin: 16rpx 24rpx 0;
    overflow: hidden;
    padding: 30rpx var(--font-size-s) 0;
    position: relative;
}

.cle {
    padding-bottom: 14rpx;
}

.cln {
    display: flex;
    justify-content: space-between;
    margin-bottom: 12rpx;
}

.clc,.cln {
    align-items: center;
}

.clc {
    display: inline-flex;
    max-width: 500rpx;
}

.clc,.clz {
    overflow: hidden;
}

.clz {
    color: var(--text-main);
    font-size: 30rpx;
    font-weight: var(--font-weight-bold);
    line-height: 30rpx;
    max-width: 460rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cl5 {
    height: 30rpx;
    width: 30rpx;
}

.clw {
    color: var(--text-third);
    font-size: var(--font-size-s);
    line-height: var(--font-size-s);
}

.cl3,.clw {
    font-weight: var(--font-weight-normal);
}

.cl3 {
    color: #ff5900;
    font-size: 26rpx;
    line-height: 26rpx;
}

.cl6 {
    color: var(--text-third);
}

.cl2 {
    color: var(--primary);
}

.cls {
    display: flex;
    justify-content: space-between;
    margin-top: 16rpx;
}

.clf {
    flex: 1;
    margin-bottom: 30rpx;
    overflow-x: hidden;
}

.cl7 {
    align-items: flex-end;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    height: 116rpx;
    justify-content: center;
    margin-left: 10rpx;
    text-align: center;
    width: 20%;
}

.clk {
    font-size: 34rpx;
    font-weight: var(--font-weight-bolder);
    line-height: 34rpx;
    margin-bottom: 14rpx;
    white-space: nowrap;
    width: 100%;
}

.clk,.clp {
    color: var(--text-main);
}

.clp {
    font-size: var(--font-size-s);
    font-weight: var(--font-weight-normal);
    line-height: var(--font-size-s);
    word-spacing: -1rpx;
}

.clp,.cly {
    display: inline-block;
}

.clo {
    color: var(--text-third);
    font-size: var(--font-size-s);
    font-weight: var(--font-weight-normal);
    line-height: var(--font-size-s);
    width: 100%;
}

.cla {
    align-items: center;
    display: flex;
    justify-content: flex-end;
    margin-bottom: 24rpx;
}

.clh {
    border-radius: 40rpx;
    color: var(--primary);
    font-size: var(--font-size-s);
    height: 64rpx;
    line-height: 64rpx;
    margin-left: 10rpx;
    min-width: 160rpx;
    padding: 0 20rpx;
    position: relative;
    text-align: center;
}

.clh,.clh::after {
    box-sizing: border-box;
}

.clh::after {
    border: 4rpx solid transparent;
    border-radius: 160rpx;
    content: "";
    height: 400%;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 0;
    transform: scale(.25);
    transform-origin: 0 0;
    width: 400%;
    z-index: 1;
}

.cl9::after {
    border-color: var(--primary);
    content: "";
}

.clu {
    background-color: var(--primary);
    color: var(--white);
}

.clv {
    background: url(https://gw.alicdn.com/tfs/TB1WvgonSslXu8jSZFuXXXg7FXa-264-192.png) no-repeat;
    background-size: 132rpx 96rpx;
    color: #666;
    font-size: 28rpx;
    line-height: 34rpx;
    padding: 31rpx 150rpx;
}

.clt {
    width: 64rpx;
}

.cld,.clt {
    height: 64rpx;
}

.cld {
    border-radius: 40rpx;
    box-sizing: border-box;
    color: var(--primary);
    font-size: var(--font-size-s);
    line-height: 64rpx;
    margin: 0 10rpx;
    text-align: center;
}

.clb {
    border-radius: 16rpx;
    margin-bottom: 30rpx;
    margin-top: 16px;
}

.cli {
    height: 165rpx;
    position: absolute;
    right: 60rpx;
    top: -30rpx;
    transform: rotate(45deg);
    width: 201rpx;
    z-index: 999;
}

.cl1 {
    margin-top: 0;
}

.clg {
    border-radius: 40rpx;
    box-sizing: border-box;
    font-size: var(--font-size-s);
    height: 64rpx;
    line-height: 64rpx;
    margin-left: 10rpx;
    padding: 0 20rpx;
    position: relative;
    text-align: center;
    width: 110rpx;
}