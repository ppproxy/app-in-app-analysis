page {
    font-family: PingFangSC-Regular;
    transition: all .3s linear 0s;
    background: #eee;
    font-size: 28rpx;
    margin-top: 0;
    -webkit-font-smoothing: antialiased;
    height: 100%;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button {
    background: inherit;
    padding: 0!important;
}

button:after {
    border: none;
}

.container {
    position: relative;
    margin: 0;
    z-index: 1;
    padding: 0 0 calc(96rpx + env(safe-area-inset-bottom));
}

.container .header-bar {
    position: fixed;
    top: 0;
    width: 100%;
    height: 170rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    background: #f9f9f9;
    box-sizing: border-box;
    border-bottom: 0.5rpx solid #dcdcdc;
    z-index: 100;
}

.container .header-bar.noactive {
    opacity: 0;
    animation: out .3s linear;
}

.container .header-bar.active {
    opacity: 1;
    animation: move .3s linear;
}

@-webkit-keyframes move {
    0% {
        opacity: 0;
    }

    30% {
        opacity: .3;
    }

    50% {
        opacity: .5;
    }

    80% {
        opacity: .8;
    }

    100% {
        opacity: 1;
    }
}

@keyframes move {
    0% {
        opacity: 0;
    }

    30% {
        opacity: .3;
    }

    50% {
        opacity: .5;
    }

    80% {
        opacity: .8;
    }

    100% {
        opacity: 1;
    }
}

@-webkit-keyframes out {
    0% {
        opacity: 1;
    }

    30% {
        opacity: .8;
    }

    50% {
        opacity: .5;
    }

    80% {
        opacity: .3;
    }

    100% {
        opacity: 0;
    }
}

@keyframes out {
    0% {
        opacity: 1;
    }

    30% {
        opacity: .8;
    }

    50% {
        opacity: .5;
    }

    80% {
        opacity: .3;
    }

    100% {
        opacity: 0;
    }
}

.container .header-bar .text {
    width: 100%;
