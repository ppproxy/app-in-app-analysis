page {
    height: 100%;
    width: 100%;
}

.wrap {
    box-sizing: border-box;
    padding-top: 120rpx;
    width: 100%;
}

.tit {
    color: #666;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    letter-spacing: 0;
    line-height: 44rpx;
    margin-top: 20rpx;
    text-align: center;
}

.icon-wrap {
    display: flex;
    flex-direction: row;
    justify-content: center;
    padding-bottom: 0;
}

.img {
    height: 100%;
    width: 100%;
}

.icon-icon {
    box-sizing: border-box;
}

.icon-img {
    border-radius: 50%;
    height: 180rpx;
    margin: 0 auto;
    width: 180rpx;
}

.icon-tit {
    color: #000;
    font-size: 42rpx;
    line-height: 58rpx;
    margin-top: 48rpx;
}

.icon-tit,.txt {
    letter-spacing: 0;
    text-align: center;
}

.txt {
    color: #666;
    font-size: 32rpx;
    line-height: 44rpx;
    margin-top: 24rpx;
    width: 630rpx;
}

.btn-wrap {
    margin-top: 48rpx;
}

.btn-cont {
    box-sizing: border-box;
    display: flex;
    padding: 0 40rpx;
}

.left {
    background: #fff;
    border: 1px solid #ccc;
    color: #9c9c9c;
    margin-top: 40rpx;
}

.left,.right {
    border-radius: 16rpx;
    font-family: PingFangSC-Medium;
    font-size: 34rpx;
    font-weight: 400!important;
    height: 96rpx;
    letter-spacing: 0;
    line-height: 92rpx;
    text-align: center;
    width: 670rpx;
}

.right {
    background: #0d8ce6;
    color: #fff;
    margin-top: 390rpx;
}

.cuo {
    line-height: 44rpx;
    padding: 24rpx 0 48rpx;
    text-align: center;
}

.cuo,.text {
    color: #666;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    letter-spacing: 0;
}

.text text {
    background: #ebebeb;
    border-radius: 50%;
    color: #000;
    display: inline-block;
    height: 42rpx;
    line-height: 42rpx;
    text-align: center;
    width: 42rpx;
}

.footer {
    bottom: 40rpx;
    left: 0;
    position: absolute;
    right: 0;
}