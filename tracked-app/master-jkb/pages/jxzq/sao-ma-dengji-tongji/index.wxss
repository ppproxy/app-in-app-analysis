}

.daochu {
    color: #1f90fb;
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    line-height: 69rpx;
    opacity: .8;
}

.imga {
    bottom: 0;
    height: 38rpx;
    margin-left: 10rpx;
    position: absolute;
    right: 94rpx;
    width: 38rpx;
}

.poster {
    background: rgba(0,0,0,.5);
    bottom: 0;
    height: 100%;
