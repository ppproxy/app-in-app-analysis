    position: fixed;
    right: 0;
    top: 0;
    width: 100%;
    z-index: 10000;
}

.cancel {
    border: none;
    border-radius: 0rpx 0rpx 16rpx 16rpx;
}

.cancel,.saveposter {
    background: #fff;
    color: #000;
    font-family: PingFangSC-Medium;
    font-size: 34rpx;
    font-weight: 400!important;
    height: 114rpx;
    letter-spacing: 0;
    line-height: 114rpx;
    text-align: center;
    width: 100%;
}

.saveposter {
    border: none;
    border-radius: 16rpx 16rpx 0 0;
}

.box1 {
    background: #fff;
}

.shanchu {
    background: #f7f7f7;
    border-radius: 0 16rpx 0 34rpx;
    color: #999;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    height: 60rpx;
    letter-spacing: 0;
    line-height: 60rpx;
