.mine,page {
    box-sizing: border-box;
}

.mine {
    background: #f3f3f3;
    height: 100%;
    overflow-x: hidden;
    padding-bottom: 180rpx;
    position: relative;
    width: 100%;
}

view.g-footer-index--g-footer {
    background: #f1f1f1;
}

.wrap1 {
    box-sizing: border-box;
    height: 100%;
    padding: 0 40rpx;
    position: relative;
    width: 100%;
    z-index: 999;
}

.ditu1 {
    height: 180rpx;
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
}

.box {
    background: #fff;
    border-top-left-radius: 16rpx;
    border-top-right-radius: 16rpx;
    box-sizing: border-box;
    margin-top: 40rpx;
    padding: 72rpx 98rpx 40rpx;
    position: relative;
}

.box,.saomaNum {
    text-align: center;
}

.saomaNum {
    display: flex;
    justify-content: space-around;
    padding: 24rpx 0;
}

.erweima {
    height: 340rpx;
    margin: auto;
    width: 340rpx;
}

.erweima,.saomaNum text {
    display: block;
}

.num {
    color: #333;
    font-family: DINAlternate-Bold;
    font-size: 64rpx;
    height: 74rpx;
    line-height: 74rpx;
}

.num,.text {
    display: inline-block;
    letter-spacing: 0;
}

.text {
    color: #999;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.allNum {
    background: #fafafa;
    display: flex;
    justify-content: space-between;
    padding: 25rpx 32rpx;
    text-align: center;
}

.back-image {
    background-image: linear-gradient(0deg,#f3f3f3,#1f90fb 68%,#1f90fb);
    height: 357rpx;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1;
}

.allNum .left,.allNum .right {
    width: 50%;
}

.smallnum {
    color: #666;
    font-family: DINAlternate-Bold;
    font-size: 28rpx;
    letter-spacing: 0;
    text-align: center;
}

.list {
    border-radius: 16rpx;
    box-shadow: 0 10rpx 20rpx 0 rgba(0,0,0,.05);
    margin-top: 40rpx;
    overflow: hidden;
}

.title {
    background: #f8f8f8;
    box-shadow: 0 2rpx 0 0 #f2f2f2;
    color: #191919;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    padding: 23rpx 0;
}

.listItem,.title {
    display: flex;
    text-align: center;
}

.listItem {
    align-items: center;
    background: #fff;
    padding: 40rpx 20rpx 40rpx 0;
}

.listItem text {
    color: #323131;
    display: block;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    text-align: center;
}

.noData {
    background: #fff;
    overflow: hidden;
    padding: 50rpx 0;
}

.noData image {
    display: block;
    margin: 0 auto;
    width: 400rpx;
}

.noData text {
    color: #86939c;
    display: block;
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    letter-spacing: 0;
    line-height: 36rpx;
    text-align: center;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.clearfix:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}

.fresh {
    background: #fff;
    border-radius: 40rpx 0 0 40rpx;
    box-shadow: 0 10rpx 20rpx 0 rgba(0,0,0,.1);
    height: 72rpx;
    line-height: 72rpx;
    position: fixed;
    right: 0;
    top: 321rpx;
    width: 168rpx;
    z-index: 2;
}

.fresh image {
    bottom: 0;
    height: 32rpx;
    left: 32rpx;
    margin: auto;
    position: absolute;
    top: 0;
    width: 32rpx;
}

.fresh text {
    margin-left: 80rpx;
}

.g-icon-arrow-down {
    display: inline!important;
}

.saomadate {
    background: #fff;
    display: flex;
    justify-content: space-between;
    padding: 32rpx;
}

.saomadate text {
    color: #000;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    line-height: 57rpx;
}

.picker {
    text-align: right;
    width: 50%;
}

.picker .view {
    border: 1px solid #ccc;
    border-radius: 34rpx;
    float: right;
    height: 54rpx;
    line-height: 54rpx;
    padding: 0 22rpx 0 32rpx;
    text-align: center;
}

.picker .view,.picker text {
    color: #999;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    letter-spacing: 0;
}

.picker text {
    line-height: 36rpx;
}

.grzx {
    padding: 0 40rpx;
}

.box .box-gr {
    background: #f7f7f7;
    border-radius: 30rpx;
    color: #1f90fb;
    display: inline-block;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    letter-spacing: 0;
    line-height: 53rpx;
    margin-bottom: 24rpx;
    max-width: 570rpx;
    padding: 0 24rpx;
    text-align: center;
}

.box1 .box-gr {
    justify-content: space-between;
    padding-bottom: 24rpx;
    text-align: center;
}

.box1 .box-gr,.grl {
    display: flex;
}

.grl {
    color: #999;
    justify-content: flex-start;
}

.grl,.grr {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    letter-spacing: 0;
    line-height: 38rpx;
}

.grr {
    color: #333;
    max-width: 342rpx;
    text-align: right;
}

.dcdy {
    align-items: center;
    background: #fff;
    display: flex;
    justify-content: space-around;
    padding: 0 51rpx 48rpx;
}

.dcdy,.dcdy view {
