.privacy {
    padding: 140rpx 40rpx 200rpx;
    box-sizing: border-box;
    width: 100vw;
    min-height: 100vh;
    background: #fff;
}

.privacy .title {
    font-size: 36rpx;
    font-weight: 700;
    margin-bottom: 30rpx;
}

.privacy .desc {
    font-size: 24rpx;
    color: #999;
    line-height: 1.3;
}

.privacy .link {
    margin-top: 40rpx;
    color: #fe7f3f;
    text-decoration: underline;
}

.btn-wrap {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100vw;
    height: 172rpx;
    padding: 36rpx;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0 -10px 20px 0 rgba(65,65,76,.1);
    display: flex;
    justify-content: space-between;
}

.btn-wrap .btn {
    width: 329rpx;
}

.btn-wrap .unagree {
    background: #fff;
    border: 1px solid #000;
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    color: #000;
}