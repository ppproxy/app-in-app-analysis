.intercept-content {
    padding-top: 58rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(180deg,#fff,#f4f5f7);
}

.intercept-content .intercept-img {
    width: 100%;
}

.goBtn {
    box-sizing: border-box;
    width: 100vw;
    position: fixed;
    z-index: 9;
    left: 0;
    bottom: 0;
    padding: 36rpx;
    background: #fff;
}