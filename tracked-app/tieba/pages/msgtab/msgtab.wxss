.empty {
    width: 452rpx;
    height: 308rpx;
    margin: 140rpx auto;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.red-point::after {
    content: "";
    position: absolute;
    right: 30px;
    width: 2.1333333333333333vw;
    height: 2.1333333333333333vw;
    margin-top: 26px;
    border-radius: 50%;
    background: #f24f18;
}

.mask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 15%;
    left: 0;
    z-index: 1;
}

.hide {
    display: none;
}

.msgtab-loading-style {
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.error-style {
    position: fixed;
    top: 280rpx;
    bottom: 0;
    width: 100%;
    background: #f5f5f5;
    text-align: center;
}

.error-style .error-image-style {
    width: 452rpx;
    margin: 0 auto;
    margin-top: 140rpx;
    text-align: center;
}

.error-style .error-text-style {
    width: 100%;
    margin-top: 100rpx;
    color: #000;
    font-size: 32rpx;
    text-align: center;
}

.no-more {
    height: 120rpx;
    background: #fff;
    color: #999;
    font-size: 12px;
    font-family: PingFangSC-Regular;
    line-height: 60px;
    text-align: center;
}