.account-container {
    background: #f5f5f5;
    color: #333;
    font-size: 16px;
    font-family: PingFangSC-Regular;
    line-height: 16px;
}

.account-tabs {
    position: relative;
    overflow: hidden;
    margin-top: -23rpx;
    margin-bottom: 12rpx;
    padding: 0 30rpx;
    border-radius: 8px 8px 0rpx 0rpx;
    background: #fff;
    box-shadow: 0 0 0 0 #f0f0f0;
}

.account-container .account-tab-item {
    padding-left: 96rpx;
    border-bottom: 1px solid #f0f0f0;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACQAAAAkBAMAAAATLoWrAAAAD1BMVEUAAABlZWVnZ2dnZ2dmZmYGsdqkAAAABHRSTlMAdrwqcCRaGgAAADNJREFUKM9jwAOYDTCETBwxhERcBNCFmFwwlamMIGUG+IQQGjGNH8aKsCQTE0d8SQ4TAACVABBTUtsCKQAAAABJRU5ErkJggg==) no-repeat right center;
    background: #fff;
    background-origin: content-box;
    background-size: 24rpx;
    line-height: 140rpx;
}

.account-name {
    overflow: hidden;
    max-width: 37%;
    color: #333;
    font-size: 20px;
    font-family: PingFangSC-Medium;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
}

.account-container .account-self {
    position: relative;
    display: flex;
    align-items: center;
    padding-bottom: 40rpx;
    padding-left: 34rpx;
    background: #fff;
    line-height: 140rpx;
}

.account-container .account-portrait {
    flex-basis: 100rpx;
    width: 100rpx;
    height: 100rpx;
    margin-right: 32rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
}

.account-container .good {
    background: url(data:image/png;base64,) no-repeat left center,url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACQAAAAkBAMAAAATLoWrAAAAD1BMVEUAAACZmZmZmZmdnZ2ZmZkR4x0rAAAABHRSTlMAdrwqcCRaGgAAADNJREFUKM9jwAOYDTCETBwxhERcBNCFmFwwlamMIGUG+IQQGjGNH8aKsCQTE0d8SQ4TAACVABBTUtsCKQAAAABJRU5ErkJggg==) no-repeat 692rpx center;
    background-color: #fff;
    background-size: 70rpx,24rpx;
}

.account-container .at {
    background: url(data:image/png;base64,) no-repeat left center,url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACQAAAAkBAMAAAATLoWrAAAAD1BMVEUAAACZmZmZmZmdnZ2ZmZkR4x0rAAAABHRSTlMAdrwqcCRaGgAAADNJREFUKM9jwAOYDTCETBwxhERcBNCFmFwwlamMIGUG+IQQGjGNH8aKsCQTE0d8SQ4TAACVABBTUtsCKQAAAABJRU5ErkJggg==) no-repeat 692rpx center;
    background-color: #fff;
    background-size: 70rpx,24rpx;
}

.account-container .complaint {
    background: url(data:image/png;base64,) no-repeat left center,url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACQAAAAkBAMAAAATLoWrAAAAD1BMVEUAAACZmZmZmZmdnZ2ZmZkR4x0rAAAABHRSTlMAdrwqcCRaGgAAADNJREFUKM9jwAOYDTCETBwxhERcBNCFmFwwlamMIGUG+IQQGjGNH8aKsCQTE0d8SQ4TAACVABBTUtsCKQAAAABJRU5ErkJggg==) no-repeat 692rpx center;
    background-color: #fff;
    background-size: 70rpx,24rpx;
}

.empty {
    width: 452rpx;
    height: 452rpx;
    margin: 140rpx auto;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    background-repeat: no-repeat;
}

.accounttab-loading-style {
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.btn {
    width: 380rpx;
    margin: 0 auto;
    padding: 25rpx 0;
    border-radius: 15rpx;
    background: #7559ff;
    color: #fff;
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    line-height: 30rpx;
    text-align: center;
}

.error-style {
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100%;
    padding-top: 60px;
    background: #fff;
    text-align: center;
}

.error-style .error-image-style {
    width: 452rpx;
    margin: 0 auto;
    margin-top: 140rpx;
    text-align: center;
}

.error-style .error-text-style {
    width: 100%;
    margin-bottom: 140rpx;
    color: #000;
    font-size: 32rpx;
    text-align: center;
}

.wx-btn {
    width: 682rpx;
    height: 80rpx;
    margin: 0 auto;
    border-radius: 8rpx;
    background: #7559ff;
    color: #fff;
    font-size: 15px;
    font-family: PingFangSC-Regular;
    line-height: 80rpx;
    text-align: center;
}

.wx-txt {
    margin-top: 78rpx;
    margin-bottom: 34rpx;
    color: #727272;
    font-size: 13px;
    font-family: PingFangSC-Regular;
    line-height: 13px;
    letter-spacing: 0;
    text-align: center;
}

.wx-hide {
    visibility: hidden;
}

.switch {
    position: absolute;
    right: 34rpx;
    width: 164rpx;
    height: 56rpx;
    margin-left: 120rpx;
    padding: 0;
    border-radius: 8rpx;
    background: #fff;
    color: #1a1a1a;
    font-size: 13px;
    font-family: PingFangSC-Regular;
    line-height: 56rpx;
    text-align: center;
    white-space: nowrap;
}

.wx-logo {
    position: absolute;
    top: 50%;
    left: 13%;
    width: 40rpx;
    height: 40rpx;
}

.account-container .no-border {
    border: none;
}

.test-modal {
    margin-top: 100rpx;
    padding: 0 4vw;
    background: #fff;
    color: #333;
}

.test-title {
    font-size: 20px;
}

.test-item {
    display: flex;
    align-items: center;
}

.test-item text {
    margin-right: 40rpx;
    font-size: 16px;
}