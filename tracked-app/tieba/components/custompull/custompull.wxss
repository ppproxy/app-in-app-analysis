.s-view {
    height: 100.1vh;
    background-color: #f5f5f5;
}

.top-pull-section,.bottom-pull-section {
    position: relative;
}

.pull-comp-bg {
    position: absolute;
    top: 0;
    display: flex;
    justify-content: center;
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}

.pull-comp-content {
    position: relative;
    background: #fff;
}

.pull-comp-loading-wrap {
    display: flex;
    height: 30px;
    padding: 5px 0;
}