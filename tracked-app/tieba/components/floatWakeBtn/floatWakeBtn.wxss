.add-btn-style-wrap {
    position: fixed;
    right: 9px;
    bottom: 50px;
}

.add-btn-style {
    width: 52px;
    height: 52px;
    padding-top: 8px;
    border-radius: 50%;
    background-image: linear-gradient(164deg,#8971ff 2%,#7358fa 48%,#7559ff 100%);
    color: #fff;
    box-shadow: 0 2px 6px 0 #7559ff;
    font-size: 13px;
    text-align: center;
}