./templates/user/user.wxssrpx./templates/pbFloors/pbBanner.wxssrpx.pb-floor-card {
    margin-top: 17px;
}

.pb-floor-card-forumdetail {
    margin-top: 35px;
}

.pb-card-container {
    padding: 0 17px;
}

.card-top {
    display: flex;
    justify-content: space-between;
    width: 100%;
}

.agree {
    display: flex;
    height: 16px;
}

.agree-num {
    margin-left: 6px;
    color: #545454;
    font-size: 13px;
    line-height: 16px;
}

.agree-num-active {
    color: #f24f18;
}

.icon {
    width: 16px;
    height: 16px;
}

.card-content-wrap {
    margin-top: 12px;
    margin-left: 47px;
    font-size: 17px;
    word-break: break-word;
}

.pb-img-container {
    margin-top: 3px;
    margin-bottom: 15px;
}

.pb-img-wrap {
    position: relative;
    display: flex;
    overflow: hidden;
    max-width: 100%;
    border-radius: 3px;
}

.pb-img-wrap image {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 3px;
    box-shadow: inset 0 0 0 1px rgba(0,0,0,.04);
}

.emoji {
    display: inline-block;
    vertical-align: middle;
    width: 16px;
    height: 16px;
}

.voice-content {
    margin: 17px 0 17px 0;
    line-height: 0;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.text {
    color: #000;
    word-break: break-word;
}

.text-copy {
    white-space: pre-wrap;
    user-select: text;
}

.god-thread-flag {
    background: url(https://tb5.bdstatic.com/xcx/pic_comment_god_big@2x.png) no-repeat 75% 0;
    background-size: 66px 41px;
}

.pb-lzl-wrap {
    margin-top: 8px;
    padding: 8px 0;
    background-color: #f5f5f5;
    font-size: 15px;
}

.floor-footer {
    padding: 0 12px 4px;
}

.bottom-content {
    overflow: hidden;
    color: #333;
    font-size: 15px;
    line-height: 23px;
    text-overflow: ellipsis;
    -webkit-line-clamp: 3;
}

.name {
    color: #8f8e94;
    font-weight: 700;
}

.del-btn {
    margin-left: 10px;
    color: #7559ff;
    font-size: 16px;
}

.pb-lzl-wrap .text {
    color: #333;
    word-break: break-word;
}

.to-lzl-btn {
    height: 15px;
    margin-top: 4px;
    padding-bottom: 18px;
    padding-left: 12px;
    color: #999;
    font-size: 15px;
    line-height: 15px;
}

.to-lzl-btn image {
    display: inline-block;
    width: 10px;
    height: 10px;
}

.beforePos {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 32px;
    margin-bottom: 17px;
    background: #f2f2f2;
    color: #a8a8a8;
    font-size: 13px;
}

.afterPos {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 32px;
    margin-top: 17px;
    background: #f2f2f2;
    color: #a8a8a8;
    font-size: 13px;
}

.bdad-wrap {
    margin-top: 17px;
    padding: 0 17px;
}

.wxad-wrap {
    display: flex;
    justify-content: center;
    margin-top: 10px;
    background: #fff;
}

.ad-track {
    display: block;
    height: 0;
}

.loading-nomore-image-style {
    width: 100px;
    height: 100px;
    margin-top: 50px;
}

.comments-no-moretxt {
    margin: 20px auto;
    text-align: center;
}

.link-wrap {
    display: inline-block;
}

.lzl-btn {
    padding: 0;
    background: transparent;
    color: #7559ff;
    font-size: 15px;
    line-height: 1;
    text-align: left;
}

.lzl-btn .left-arrow {
    display: inline-block;
    vertical-align: middle;
    width: 12rpx;
    height: 12rpx;
    margin-top: -6rpx;
    border-top: 1px solid #4884d4;
    border-left: 1px solid #4884d4;
    transform: rotate(135deg);
}