.frs-void-body-container {
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}

.frs-void-body-container .frs-void-body-top {
    position: relative;
    width: 100%;
    height: 382rpx;
}

.frs-void-body-container .frs-void-body-top .frs-top-backicon {
    position: absolute;
    top: 60rpx;
    left: 34rpx;
    width: 44rpx;
    height: 44rpx;
    transform: rotate(180deg);
}

.frs-void-body-container .frs-void-body-toplist .toplist-body {
    width: 682rpx;
    height: 76px;
    margin: 0 34rpx;
    border-radius: 16rpx;
    background: #fff;
    box-shadow: 0 2px 6px 0 rgba(0,0,0,.08);
}

.frs-void-body-container .frs-void-body-toplist .toplist-legends {
    display: flex;
    justify-content: center;
    margin-top: 18rpx;
}

.frs-void-body-container .frs-void-body-toplist .toplist-legends .toplist-legends-item {
    width: 30rpx;
    height: 4rpx;
    border-radius: 1px;
    background: #d4d4d4;
}

.frs-void-body-container .frs-void-body-toplist .toplist-legends .active {
    background: #808080;
}

.frs-void-body-container .frs-void-body-content {
    margin-top: 24rpx;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-filter {
    width: 100%;
    height: 82rpx;
    margin-bottom: 1px;
    border-radius: 14px 14px 2px 2px;
    background: #fff;
    box-shadow: 0 0 0 0 #f0f0f0;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists {
    padding: 0 34rpx;
    background: #fff;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .body-lists-sort-wrap {
    display: flex;
    justify-content: space-between;
    padding-top: 28rpx;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .body-lists-sort {
    width: 142rpx;
    height: 24rpx;
    background: #f2f2f2;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-body-users {
    display: flex;
    align-items: center;
    margin-top: 50rpx;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-user-logo {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    background: #f2f2f2;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-user-name {
    width: 160rpx;
    height: 30rpx;
    margin-left: 24rpx;
    background: #f2f2f2;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-body-title-1 {
    width: 100%;
    height: 30rpx;
    margin-top: 24rpx;
    background: #f2f2f2;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-body-title-2 {
    width: 100%;
    height: 30rpx;
    margin-top: 16rpx;
    margin-bottom: 20rpx;
    background: #f2f2f2;
}

.frs-void-body-container .frs-void-body-content .frs-void-body-lists .frs-void-body-images {
    width: 100%;
    height: 350rpx;
    border-radius: 12rpx;
    background: #f2f2f2;
}