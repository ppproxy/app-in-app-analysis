.error-wrap {
    color: #333;
    font-size: 16px;
    text-align: center;
}

.error-wrap .error-icon {
    width: 154px;
    height: 154px;
    margin: 20px auto 0 auto;
    margin-top: 52px;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.error-wrap .error-msg {
    margin-top: 28px;
    margin-bottom: 47px;
}

.error-wrap .error-btn {
    width: 286px;
    height: 44px;
    margin: 20px auto 0 auto;
    border-radius: 2px;
    background-image: linear-gradient(90deg,#2bb8ff 0%,#2b80ff 100%);
    color: #fff;
    font-size: 15px;
    line-height: 44px;
}

.error-wrap .error-btn::after {
    border: 0 none;
}