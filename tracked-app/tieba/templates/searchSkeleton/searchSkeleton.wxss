.all-bg {
    overflow: hidden;
    width: 100%;
    height: 100vh;
    background: #fff;
}

.input-bg {
    width: 100%;
    height: 44px;
    background: #f5f5f5;
}

.search-tip {
    display: flex;
    margin-top: 12px;
    padding: 0 17px;
}

.search-tip-image {
    width: 60px;
    height: 60px;
    margin-right: 8px;
}

.bg-color {
    background: #f5f5f5;
}

.search-tip-con {
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: space-between;
    padding: 2px 0;
}

.search-tipCon-one {
    width: 60px;
    height: 16px;
}

.search-tipCon-two {
    width: 100%;
    height: 12px;
}

.search-tipCon-three {
    width: 154px;
    height: 12px;
}

.search-mini-con {
    display: flex;
    flex-wrap: wrap;
    padding: 0 17px;
}

.search-mini {
    display: flex;
    margin-top: 20px;
    margin-right: 15px;
}

.search-mini-image {
    width: 40px;
    height: 40px;
}

.search-mini-titleCon {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 8px;
    padding: 2px 0;
}

.search-mini-one {
    width: 80px;
    height: 16px;
}

.search-mini-two {
    width: 80px;
    height: 12px;
}

.single {
    margin-top: 20px;
    margin-left: 17px;
}

.split-line {
    width: 100%;
    height: 10px;
    margin-top: 20px;
}

.card-con {
    display: flex;
    padding: 17px;
}

.card-logo {
    width: 35px;
    height: 35px;
    border-radius: 50%;
}

.card-cont {
    width: 100%;
    height: 18px;
    margin: 0 17px;
}

.card-image-wrap {
    margin-top: 10px;
    padding: 0 17px;
}

.card-image {
    width: 100%;
    height: 192px;
}