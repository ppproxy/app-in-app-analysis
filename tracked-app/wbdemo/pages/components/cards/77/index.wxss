.card-77 {
    position: relative;
    box-sizing: border-box;
    background: #fff;
    border-bottom: .5px solid #dcdcdc;
}

.title-left-color {
    position: absolute;
    left: 0;
    width: 6rpx;
    height: 100%;
}

.content {
    display: flex;
    padding: 20rpx;
    align-items: center;
}

.start-time {
    font-size: 32rpx;
    color: #939393;
    margin: 0 30rpx 0 10rpx;
}

.content-text {
    width: 70%;
}

.title {
    max-width: 100%;
    font-size: 28rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.desc {
    font-size: 24rpx;
    margin-top: 20rpx;
}

.order-button {
    position: absolute;
    right: 20rpx;
    background: #eb893c;
    padding: 3px 8px;
    color: #fff;
    border-radius: 3px;
    font-size: 24rpx;
}