.card-9 {
    background: #fff;
}

.card-9,.card-type {
    box-sizing: border-box;
    border-bottom: .5px solid #e5e5e5;
}

.card-type {
    padding: 12rpx 20rpx;
    overflow: hidden;
    font-size: 28rpx;
}

.card-type-title {
    font-size: 26rpx;
    line-height: 38rpx;
    color: #8e8e8e;
}

.icon-down {
    position: absolute;
    right: 20rpx;
    font-size: 26rpx;
    vertical-align: middle;
    transform: rotate(-90deg);
    color: #939393;
}

.wb-detail {
    padding: 25rpx 25rpx 0;
}

.wb-header {
    padding-bottom: 20rpx;
    display: flex;
}

.wb-header .text {
    vertical-align: top;
    margin-left: 20rpx;
    flex: 1;
}

.wb-header .text .name {
    display: flex;
    align-items: center;
    font-size: 32rpx;
    color: #f36100;
    line-height: 44rpx;
}

.wb-header .text .msg {
    margin-top: 6rpx;
    font-size: 24rpx;
    color: #898989;
}

.wb-header .text .msg .source {
    display: inline-block;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    vertical-align: top;
}

.wb-header .attent {
    padding: 0 18rpx;
    font-size: 22rpx;
    color: #fe8c15;
    border: 1px solid #fe8c15;
    line-height: 56rpx;
    height: 56rpx;
    border-radius: 28rpx;
}

.wb-header .attent .em {
    font-size: 42rpx;
    vertical-align: top;
    line-height: 48rpx;
}

.wb-content {
    padding-bottom: 10rpx;
}

.wb-content .text {
    line-height: 46rpx;
    color: #333;
    font-size: 32rpx;
}

.wb-content .text .emotion {
    display: inline-block;
    width: 30rpx;
    vertical-align: top;
    margin-top: 10rpx;
}

.wb-content .text .icon {
    display: inline-block;
    width: 30rpx;
    vertical-align: middle;
    margin: -4rpx 6rpx 0 10rpx;
}

.wb-content .parse-text {
    display: inline;
    line-height: 46rpx;
    color: #333;
    font-size: 32rpx;
    word-break: break-all;
}

.wb-content .parse-text.topic {
    color: #4b73a0;
}

.wb-content .parse-text.emotion {
    display: inline-block;
    vertical-align: text-top;
    margin: 0 5rpx;
    width: 32rpx;
    height: 32rpx;
}

.wb-content .media {
    padding-top: 10rpx;
}

.wb-content .media .ul {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    overflow: hidden;
}

.wb-content .media .li {
    box-sizing: border-box;
    float: left;
    width: 33.3%;
    height: 226rpx;
    padding: 0 10rpx 10rpx 0;
}

.wb-content .media .ul.four {
    width: 70%;
}

.wb-content .media .li:nth-child(3) {
    margin-right: 0;
}

.wb-content .media .li .img {
    width: 100%;
    display: block;
    height: 100%;
    background: #ebebeb;
}

.wb-content .media .video,.wb-content .media .videofeed {
    width: 100%;
    display: block;
    height: 394rpx;
}

.wb-content .media .videofeed {
    position: relative;
}

.wb-content .media .videofeed .img {
    background: #ebebeb;
    width: 100%;
    display: block;
    height: 100%;
}

.wb-content .media .videofeed .pause {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 128rpx;
    height: 128rpx;
}

.wb-content .media .videofeed .during {
    position: absolute;
    top: 50%;
    left: 50%;
    color: #fff;
    font-size: 32rpx;
    transform: translateX(-50%);
}

.wb-content .media .article {
    position: relative;
}

.wb-content .media .article .img {
    width: 100%;
    display: block;
    height: 394rpx;
    background: #ebebeb;
}

.wb-content .media .article .msg {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    color: #fff;
    padding: 18rpx 22rpx 0 30rpx;
    font-size: 24rpx;
    line-height: 34rpx;
}

.wb-content .media .article .avator {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    border-radius: 20rpx;
    vertical-align: top;
    margin-right: 12rpx;
}

.wb-content .media .article .focus {
    border: 1px solid #fff;
    display: inline-block;
    padding: 0 20rpx;
    line-height: 26rpx;
    height: 32rpx;
    border-radius: 32rpx;
    margin-left: 10rpx;
    font-size: 18rpx;
}

.wb-content .media .article .focus .icon {
    font-size: 24rpx;
    display: inline-block;
    margin-right: 6rpx;
}

.wb-content .media .article .logo {
    float: right;
    background-image: url(https://h5.sinaimg.cn/upload/2017/03/07/319/icon-acl.svg);
    background-size: 70rpx auto;
    background-repeat: no-repeat;
    color: transparent;
    background-position: 0 -60rpx;
}

.wb-content .media .article .title {
    position: absolute;
    bottom: 22rpx;
    left: 30rpx;
    right: 50rpx;
    font-size: 30rpx;
    line-height: 40rpx;
}

.wb-content .media .supertopic {
    background-color: #f8f8f8;
    display: flex;
}

.wb-content .media .webpage {
    overflow: hidden;
}

.wb-content .media .webpage .img {
    width: 150rpx;
    height: 150rpx;
    float: left;
    background: #ebebeb;
}

.wb-content .media .webpage .webpage-content {
    box-sizing: border-box;
    padding: 20rpx 20rpx 20rpx 170rpx;
    background: #f8f8f8;
}

.wb-content .media .webpage .webpage-content .display_name {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.wb-content .media .webpage .webpage-content .summary {
    font-size: 24rpx;
    color: #999;
    margin-top: 10rpx;
}

@font-face {
    font-family:marvelfont;src:url(data:font/woff;base64,) format("woff");font-weight:400;font-style:normal;
}

.m-font {
    font-family: marvelfont;
    display: inline-block;
    -webkit-font-smoothing: antialiased;
    font-size: 32rpx;
}

.wb-comment {
    padding-left: 10rpx;
    font-size: 24rpx;
}

.comment-list {
    margin-bottom: 10rpx;
    max-width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.comment-name {
    color: #60769b;
    vertical-align: middle;
}

.comment-text {
    color: #646464;
}

.comment-text,.comment-text .emotion {
    display: inline-block;
    vertical-align: middle;
}

.comment-text .emotion {
    margin-left: 5rpx;
    width: 30rpx;
    height: 30rpx;
}

.wb-footer {
    display: flex;
    box-sizing: border-box;
    align-items: center;
    justify-content: space-between;
    border-top: .5px solid #dcdcdc;
}

.wb-footer .footer-left {
    display: flex;
    justify-content: flex-start;
}

.wb-footer .comment,.wb-footer .forward,.wb-footer .like {
    justify-content: space-between;
    flex: 1;
    width: 180rpx;
    text-align: center;
    font-size: 24rpx;
    color: #8e8e8e;
    line-height: 74rpx;
    height: 74rpx;
}

.wb-footer .m-font {
    vertical-align: middle;
    margin-right: 14rpx;
}

.wb-footer .forward .m-font::before {
    content: "\E004";
}

.wb-footer .comment .m-font::before {
    content: "\E003";
}

.wb-footer .like .m-icon {
    display: inline-block;
    vertical-align: middle;
    width: 32rpx;
    height: 32rpx;
    background-image: url(https://h5.sinaimg.cn/marvel/v1.4.5/img/sprite.svg);
    background-size: 90rpx 1710rpx;
    background-position: -30rpx -750rpx;
    margin-right: 12rpx;
}

.wb-footer .forward-share {
    width: 180rpx;
    height: 74rpx;
    line-height: 74rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #8e8e8e;
}

.wb-share {
    justify-content: flex-end;
}

.wb-share .bar-item-wechat {
    width: 120rpx;
    height: 74rpx;
    line-height: 74rpx;
    display: flex;
    align-items: center;
    font-weight: 500;
}

.wb-share .bar-icon-wechat {
    display: block;
    width: 40rpx;
    height: 40rpx;
}

.wb-share .bar-item-txt-wechat {
    font-size: 22rpx;
    font-family: PingFangSC-Regular;
    color: #8e8e8e;
    margin-left: 10rpx;
}

.retweeted-blog {
    padding: 10rpx 25rpx 0;
    background-color: #f7f7f7;
}

.richstyle {
    color: #4f7daf;
    font-style: normal;
}

.image_box {
    width: 100%;
    height: 100%;
    background: #ebebeb;
}