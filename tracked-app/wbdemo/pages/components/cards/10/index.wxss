.card-10 {
    box-sizing: border-box;
    padding: 20rpx;
    background: #fff;
    border-bottom: .5px solid #dcdcdc;
    overflow: hidden;
}

.user-img {
    position: relative;
    float: left;
    width: 130rpx;
    height: 130rpx;
}

.img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    border: .5px solid #f0f0f0;
    box-sizing: border-box;
    background: #ebebeb;
}

.screen-name {
    position: relative;
    font-size: 36rpx;
    padding: 10rpx 0 0 160rpx;
}

.desc1 {
    color: #333;
}

.desc1,.followers-count {
    font-size: 24rpx;
    padding-left: 30rpx;
    margin-top: 5rpx;
    max-width: 80%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.followers-count {
    color: #939393;
}