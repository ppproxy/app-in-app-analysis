<view class="custom data-v-79ba19e8">
    <view class="custom-space data-v-79ba19e8" style="{{'height:'+height+';'}}"></view>
    <view class="custom-bar data-v-79ba19e8" style="{{'height:'+height+';'}}">
        <view class="custom-left flex-box vertical-center data-v-79ba19e8">
            <view bindtap="__e" class="custom-icon1 flex-shrink0 data-v-79ba19e8" data-event-opts="{{[ [ 'tap',[ [ 'back',['$event'] ] ] ] ]}}">
                <image class="image data-v-79ba19e8" src="https://eciapp-img.qichacha.com/community/222eef02-8f0b-459d-a1f8-f75997bcba3c1630900955560.png"></image>
            </view>
            <view bindtap="__e" class="custom-icon2 flex-shrink0 data-v-79ba19e8" data-event-opts="{{[ [ 'tap',[ [ 'e0',['$event'] ] ] ] ]}}">
